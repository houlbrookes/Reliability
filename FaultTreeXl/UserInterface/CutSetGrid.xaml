<Window x:Class="FaultTreeXl.UserInterface.CutSetGrid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FaultTreeXl.UserInterface"
        mc:Ignorable="d" d:DataContext="{DynamicResource CutSetGridContext}"
        Title="Cut Sets" Height="450" Width="800">
    <Window.Resources>
        <local:CutSetGridContext x:Key="CutSetGridContext" />
    </Window.Resources>
    <Grid>
        <DataGrid ItemsSource="{Binding Path=Data}">
        </DataGrid>
    </Grid>
</Window>
